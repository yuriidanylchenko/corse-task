<Window x:Class="course_task.WindowLog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:course_task"
        mc:Ignorable="d"
        Title="WindowLog" Height="300" Width="300">
    <Grid>
        <Button x:Name="buttonLog" Content="Log-in" HorizontalAlignment="Left" Margin="76,176,0,0" VerticalAlignment="Top" Width="75" Click="buttonLog_Click"/>
        <Button x:Name="buttonClose" Content="Close" HorizontalAlignment="Left" Margin="166,176,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox x:Name="textBoxEmail" HorizontalAlignment="Left" Height="23" Margin="91,100,0,0" TextWrapping="Wrap" Text="email" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBoxPassword" HorizontalAlignment="Left" Height="23" Margin="91,128,0,0" TextWrapping="Wrap" Text="password" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
